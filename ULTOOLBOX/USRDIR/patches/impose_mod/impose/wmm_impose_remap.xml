	<View id="remove_impose_remap_item">
		<Attributes>
			<Table key="remove_impose_remap">
				<Pair key="icon"><>/dev_hdd0/game/ULTOOLBOX/USRDIR/icon/delete_hdd.png</></Pair>
				<Pair key="title"><>Remove Impose Resources</></Pair>
				<Pair key="info"><>Delete these webMAN MOD related resources from HDD</></Pair>
			</Table>
		</Attributes>
		<Items>
			<Query class="type:x-xmb/folder-pixmap" key="remove_impose_remap" attr="remove_impose_remap" src="#remove_impose_remap_action_item"/>
		</Items>
	</View>

	<View id="remove_impose_remap_action_item">
		<Attributes>
			<Table key="remove_impose_remap_action">
				<Pair key="icon"><>/dev_hdd0/game/ULTOOLBOX/USRDIR/icon/warning_folder.png</></Pair>
				<Pair key="title"><>Delete these wMM Related Resources?</></Pair>
				<Pair key="info"><>Press  to continue, Press  to go back.

				This deletes the dev_hdd0/tmp/impose/ folder that is used by wMM</></Pair>
				<Pair key="module_name"><>videodownloader_plugin</></Pair>
				<Pair key="module_action"><>fdelete#/dev_hdd0/tmp/impose</></Pair>
				<Pair key="bar_action"><>none</></Pair>
				<Pair key="lbl_half"><>1</></Pair>
			</Table>
		</Attributes>
		<Items>
			<Item class="type:x-xmb/module-action" key="remove_impose_remap_action" attr="remove_impose_remap_action"/>
		</Items>
	</View>

